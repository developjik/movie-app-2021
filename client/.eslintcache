C:\\Users\\developjik\\Desktop\\2021\\react_mongo_movie\\client\\src\\_reducers\\index.js",[],"C:\\Users\\developjik\\Desktop\\2021\\react_mongo_movie\\client\\src\\hoc\\auth.js",["145","146"],"import React, { useEffect } from \"react\";\r\nimport { useDispatch } from \"react-redux\";\r\n\r\nimport { auth } from \"../_actions/user_action\";\r\n\r\nexport default function (Component, option, adminRoute = null) {\r\n  // option 1. null => 아무나 출입 가능 2. true => 로그인한 유저만 출입 가능 3. false => 로그인한 유저는 출입 불가능\r\n  function AuthenticationCheck(props) {\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n      dispatch(auth()).then((response) => {\r\n        if (!response.payload.isAuth) {\r\n          // no login\r\n          if (option) {\r\n            props.history.push(\"/login\");\r\n          }\r\n        } else {\r\n          // login\r\n          if (adminRoute && !response.payload.isAdmin) {\r\n            props.history.push(\"/\");\r\n          }\r\n          if (option === false) {\r\n            props.history.push(\"/\");\r\n          }\r\n        }\r\n      });\r\n    }, []);\r\n\r\n    return <Component />;\r\n  }\r\n\r\n  return AuthenticationCheck;\r\n}\r\n","C:\\Users\\developjik\\Desktop\\2021\\react_mongo_movie\\client\\src\\_reducers\\user_reducer.js",["147"],"C:\\Users\\developjik\\Desktop\\2021\\react_mongo_movie\\client\\src\\components\\views\\LandingPage\\LandingPage.js",["148","149","150"],"import axios from \"axios\";\r\nimport React, { useState, useEffect } from \"react\";\r\nimport { withRouter } from \"react-router-dom\";\r\n\r\nimport { API_URL, API_KEY, IMAGE_BASE_URL } from \"../../../config/config\";\r\n\r\nimport CarouselCards from \"./Sections/CarouselCards\";\r\nimport GridCards from \"./Sections/GridCards\";\r\n\r\nimport { Row, Button, Carousel,BackTop } from \"antd\";\r\nimport { DownSquareOutlined } from \"@ant-design/icons\";\r\n\r\nfunction LandingPage(props) {\r\n  const [movies, setMovies] = useState([]);\r\n  const [mainMovieImage, setMainMovieImage] = useState(null);\r\n  const [currentPage, setCurrentPage] = useState(0);\r\n\r\n  const fetchMovies = (endpoint) => {\r\n    fetch(endpoint)\r\n      .then((response) => response.json())\r\n      .then((response) => {\r\n        setMovies([...movies, ...response.results]);\r\n        setMainMovieImage(response.results[0]);\r\n        setCurrentPage(response.page);\r\n      });\r\n  };\r\n\r\n  const onLoadMoreMovies = () => {\r\n    const endpoint = `${API_URL}movie/popular?api_key=${API_KEY}&language=ko_KR&page=${\r\n      currentPage + 1\r\n    }`;\r\n    fetchMovies(endpoint);\r\n  };\r\n\r\n  useEffect(() => {\r\n    const endpoint = `${API_URL}movie/popular?api_key=${API_KEY}&language=ko_KR&page=1`;\r\n    fetchMovies(endpoint);\r\n  }, []);\r\n\r\n  return (\r\n    <div style={{ width: \"100%\", margin: \"0\" }}>\r\n      {/* Main image Carousel*/}\r\n      <Carousel autoplay>\r\n        {movies &&\r\n          movies.map((movie, index) => (\r\n            <CarouselCards\r\n              key={index}\r\n              image={\r\n                movie.poster_path\r\n                  ? `${IMAGE_BASE_URL}w500${movie.poster_path}`\r\n                  : null\r\n              }\r\n              movieId={movie.id}\r\n              title={movie.original_title}\r\n              description={movie.overview}\r\n            />\r\n          ))}\r\n      </Carousel>\r\n      <div style={{ width: \"85%\", margin: \"1rem auto\" }}>\r\n        <h2>Movies by latest</h2>\r\n        <hr />\r\n        {/* movie grid cards */}\r\n        <Row gutter={[16, 16]}>\r\n          {movies &&\r\n            movies.map((movie, index) => (\r\n              <GridCards\r\n                key={index}\r\n                image={\r\n                  movie.poster_path\r\n                    ? `${IMAGE_BASE_URL}w500${movie.poster_path}`\r\n                    : null\r\n                }\r\n                movieId={movie.id}\r\n                movieName={movie