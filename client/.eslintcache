],"C:\\Users\\developjik\\Desktop\\2021\\boiler_plate\\client\\src\\components\\views\\RegisterPage\\RegisterPage.js",[],"C:\\Users\\developjik\\Desktop\\2021\\boiler_plate\\client\\src\\components\\views\\Footer\\Footer.js",[],"C:\\Users\\developjik\\Desktop\\2021\\boiler_plate\\client\\src\\_reducers\\index.js",[],"C:\\Users\\developjik\\Desktop\\2021\\boiler_plate\\client\\src\\_actions\\user_action.js",[],"C:\\Users\\developjik\\Desktop\\2021\\boiler_plate\\client\\src\\_reducers\\user_reducer.js",["139"],"const initialState = null;\r\n\r\nexport default function (prevState = initialState, action) {\r\n  switch (action.type) {\r\n    case \"LOGIN\":\r\n      return {\r\n        ...prevState,\r\n        loginSuccess: action.payload,\r\n      };\r\n    case \"REGISTER\":\r\n      return {\r\n        ...prevState,\r\n        registerSuccess: action.payload,\r\n      };\r\n    case \"AUTH\":\r\n      return {\r\n        ...prevState,\r\n        userData: action.payload,\r\n      };\r\n    default:\r\n      return prevState;\r\n  }\r\n}\r\n","C:\\Users\\developjik\\Desktop\\2021\\boiler_plate\\client\\src\\hoc\\auth.js",["140","141"],"import React, { useEffect } from \"react\";\r\nimport { useDispatch } from \"react-redux\";\r\n\r\nimport { auth } from \"../_actions/user_action\";\r\n\r\nexport default function (Component, option, adminRoute = null) {\r\n  // option 1. null => 아무나 출입 가능 2. true => 로그인한 유저만 출입 가능 3. false => 로그인한 유저는 출입 불가능\r\n  function AuthenticationCheck(props) {\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n      dispatch(auth()).then((response) => {\r\n        if (!response.payload.isAuth) {\r\n          // no login\r\n          if (option) {\r\n            props.history.push(\"/login\");\r\n          }\r\n        } else {\r\n          // login\r\n          if (adminRoute && !response.payload.isAdmin) {\r\n            props.history.push(\"/\");\r\n          }\r\n          if (!option) {\r\n            props.history.push(\