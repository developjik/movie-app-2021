nction AuthenticationCheck(props) {\r\n    const dispatch = useDispatch();\r\n\r\n    useEffect(() => {\r\n      dispatch(auth()).then((response) => {\r\n        if (!response.payload.isAuth) {\r\n          // no login\r\n          if (option) {\r\n            props.history.push(\"/login\");\r\n          }\r\n        } else {\r\n          // login\r\n          if (adminRoute && !response.payload.isAdmin) {\r\n            props.history.push(\"/\");\r\n          }\r\n          if (option === false) {\r\n            props.history.push(\"/\");\r\n          }\r\n        }\r\n      });\r\n    }, []);\r\n\r\n    return <Component />;\r\n  }\r\n\r\n  return AuthenticationCheck;\r\n}\r\n","C:\\Users\\developjik\\Desktop\\2021\\react_mongo_movie\\client\\src\\_reducers\\user_reducer.js",["143"],"C:\\Users\\developjik\\Desktop\\2021\\react_mongo_movie\\client\\src\\components\\views\\LandingPage\\LandingPage.js",["144","145","146"],"import axios from \"axios\";\r\nimport React, { useState, useEffect } from \"react\";\r\nimport { withRouter } from \"react-router-dom\";\r\n\r\nimport { API_URL, API_KEY, IMAGE_BASE_URL } from \"../../../config/config\";\r\n\r\nimport CarouselCards from \"./Sections/CarouselCards\";\r\nimport GridCards from \"./Sections/GridCards\";\r\n\r\nimport { Row, Button, Carousel } from \"antd\";\r\nimport { DownSquareOutlined } from \"@ant-design/icons\";\r\n\r\nfunction LandingPage(props) {\r\n  const [movies, setMovies] = useState([]);\r\n  const [mainMovieImage, setMainMovieImage] = useState(null);\r\n  const [currentPage, setCurrentPage] = useState(0);\r\n\r\n  const fetchMovies = (endpoint) => {\r\n    fetch(endpoint)\r\n      .then((response) => response.json())\r\n      .then((response) => {\r\n        setMovies([...movies, ...response.results]);\r\n        setMainMovieImage(response.results[0]);\r\n        setCurrentPage(response.page);\r\n      });\r\n  };\r\n\r\n  const onLoadMoreMovies = () => {\r\n    const endpoint = `${API_URL}movie/popular?a